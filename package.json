{
  "name": "postgres-express-node",
  "version": "1.0.0",
  "description": "Setting up a todo list backend",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "test": "NODE_ENV=test npm run reset:db && nyc mocha server/tests/*.spec.js",
    "migrate": "sequelize db:migrate",
    "reset:db": "sequelize db:migrate:undo:all && npm run migrate",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postinstall": "sequelize db:migrate"
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "exclude": [
      "**/*.d.js",
      "**/*.spec.js",
      "coverage",
      "dist",
      "server/tests",
      "server/migrations"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "keywords": [],
  "author": "Ryan Wire",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "celebrate": "^10.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.8.9",
    "sequelize-cli": "5.5.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.6",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0",
    "sinon-express-mock": "^2.2.0"
  }
}
