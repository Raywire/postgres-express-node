{
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "NODE_ENV": {
      "required": true
    },
    "PORT": {
      "required": true
    },
    "SECRET_KEY": {
      "required": true
    },
    "DATABASE_URL": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "postgres-express-node",
  "scripts": {
  },
  "stack": "heroku-18"
}
